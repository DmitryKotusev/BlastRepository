{
    "extends": "airbnb-base",
    "rules": {
      "no-unused-vars": 0,
      "linebreak-style": "off",
      "func-names": "off",
      "prefer-arrow-callback": 0,
      "prefer-const": 0,
      "no-param-reassign": "off",
      "no-await-in-loop": 0
    },
    "globals": {
      "user": true,
      "Photopost": true,
      "users": true,
      "latestSkip": true,
      "latestTop": true,
      "latestFilterConfig": true,
      "view": true,
      "controller": true,
      "model": true,
      "arrayOfSubcribers": true,
      "currentState": true,
      "currentName": true,
      "statesMassive": true
    },
    "env": {
      "browser": true
    }
}